{
  "manifest_version": 3,
  "name": "Italian License Quiz Shortcuts",
  "version": "1.0",
  "description": "Keyboard shortcuts for quizpatenteonline.it to speed up studying",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.quizpatenteonline.it/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
